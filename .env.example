# Path to your diploma project directory
# Example: /Users/username/Documents/diploma
WORKING_DIR=/path/to/your/diploma/project

# Server configuration (optional)
# Port for development server
PORT=3000

# LiveReload configuration (optional)
# Port for LiveReload server
LIVERELOAD_PORT=35729

# Validation settings (optional)
# Maximum length for commit messages
MAX_COMMIT_LENGTH=60

# Image optimization settings (optional)
# Quality for JPEG compression (1-100)
JPEG_QUALITY=80
# Quality for WebP compression (1-100)
WEBP_QUALITY=75
# PNG compression level (1-9)
PNG_COMPRESSION=9 